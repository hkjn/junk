[Unit]
Description=Dev version of my junk app
After=docker.service

[Service]
TimeoutStartSec=0
ExecStartPre=-/usr/bin/docker kill junkapp-dev
ExecStartPre=-/usr/bin/docker rm junkapp-dev
ExecStartPre=/usr/bin/docker pull hkjn/junkapp:v1
ExecStart=/usr/bin/docker run -p 127.0.0.1:8910:8910 --name junkapp-dev -t hkjn/junkapp:v1
ExecStop=/usr/bin/docker stop junkapp-dev

[X-Fleet]
Conflicts=junkapp-dev.service

